{
  "name": "ytk6565.net",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "pnpm": ">=7"
  },
  "pnpm": {
    "overrides": {
      "vite": "3.2.4",
      "rollup": "2.79.1",
      "react": "18.2.0",
      "react-dom": "18.2.0"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "@testing-library/dom"
      ]
    }
  },
  "packageManager": "pnpm@7.15.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "dev": "turbo run dev --parallel",
    "prepare": "run-p husky contentful-typescript-codegen",
    "husky": "husky install",
    "contentful-typescript-codegen": "pnpm --filter @ytk6565.net/infrastructure contentful-typescript-codegen"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "eslint": "8.28.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "turbo": "1.6.3",
    "zod": "3.19.1"
  }
}

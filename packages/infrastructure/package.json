{
  "name": "@ytk6565.net/infrastructure",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    "./dist/*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.js"
    }
  },
  "scripts": {
    "dev": "npm-run-all --serial clean --parallel 'build:* -w'",
    "build": "npm-run-all --serial clean build:*",
    "build:generate": "tsc -p tsconfig.build.json",
    "build:resolve-alias": "tsc-alias -p tsconfig.build.json",
    "clean": "rimraf dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.tsx, --ignore-path .gitignore .",
    "contentful-typescript-codegen": "node ./config/contentful/typescript-codegen/command.cjs --output src/types/generated/contentful.d.ts"
  },
  "dependencies": {
    "@ytk6565.net/domain": "workspace:*"
  },
  "devDependencies": {
    "@contentful/rich-text-types": "15.14.1",
    "@vitest/coverage-c8": "0.25.3",
    "@ytk6565.net/eslint-config-base": "workspace:*",
    "@ytk6565.net/tsconfig": "workspace:*",
    "contentful": "9.2.13",
    "contentful-management": "10.21.5",
    "contentful-typescript-codegen": "3.3.0",
    "dotenv": "16.0.3",
    "happy-dom": "7.7.0",
    "msw": "0.49.0",
    "rimraf": "3.0.2",
    "tsc-alias": "1.7.1",
    "typescript": "4.8.4",
    "vite-tsconfig-paths": "3.5.2",
    "vitest": "0.25.1"
  }
}

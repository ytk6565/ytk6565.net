    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@vitejs/plugin-react": "2.2.0",
    "@ytk6565.net/eslint-config-react": "workspace:*",
    "@ytk6565.net/stylelint-config-base": "workspace:*",
    "@ytk6565.net/tsconfig": "workspace:*",
    "stylelint": "14.15.0",
    "typescript": "4.8.4",
    "vite": "3.2.3",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-fonts": "0.6.0",
    "vite-plugin-radar": "0.5.0",
    "vite-tsconfig-paths": "3.5.2"
  }
